<?xml version="1.0"?>
<Application name="magneto">
  
   <Page bearsData="true" name="Home" title="" index="">
		<Link name="SignIn" type="linktext"
			locator="Sign In" dataField="false" anchor="true">
		</Link>	
		<Link name="Men" type="xpath"
			locator="//a/span[contains(text(),'Men')]" dataField="false" anchor="true">
		</Link>	
		<Link name="Women" type="xpath"
			locator="//a/span[contains(text(),'Women')]" dataField="false" anchor="true">
		</Link>	
		<Link name="whatsnew" type="xpath"
			locator="//a/span[contains(text(),'What')]" dataField="false" anchor="true">
		</Link>	
		<Link name="Gear" type="xpath"
			locator="//a/span[contains(text(),'Gear')]" dataField="false" anchor="true">
		</Link>	
		<Link name="Training" type="xpath"
			locator="//a/span[contains(text(),'Training')]" dataField="false" anchor="true">
		</Link>	
		<Link name="Sale" type="xpath"
