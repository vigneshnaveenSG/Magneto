		</Link>	
		<WebElement name="Hotsellers" type="xpath"
			locator="//h2[contains(text(),'Hot Sellers')]" dataField="false" anchor="true">
		</WebElement>
		<WebElement name="Hotseller" type="xpath"
			locator="//div[@class='product-item-details']//child::*[@title='itemvalue']" dataField="false" anchor="true">
		</WebElement>
		<WebElement name="sizes" type="xpath"
			locator="//div[@class='product-item-details']//child::*[@title='itemvalue']/..//following-sibling::div/*[@class='swatch-attribute size']/*/div[contains(text(),'itemvalue')]" dataField="false" anchor="true">
		</WebElement>
		<WebElement name="colours" type="xpath"
			locator="//div[@class='product-item-details']//child::*[@title='itemvalue']/..//following-sibling::div/*[@class='swatch-attribute color']/*/div[@option-label='itemvalue']" dataField="false" anchor="true">
		</WebElement>
		<WebElement name="review" type="xpath"
			locator="//div[@class='product-item-details']//child::*[@title='itemvalue']/..//following-sibling::div/*[@class='itemvalue']" dataField="false" anchor="true">
		</WebElement>		
		<WebElement name="lowestprice" type="xpath"
			locator="//div[@class='product-item-details']//child::*[@title='itemvalue']/..//following-sibling::div[@class='price-box price-final_price']//span[contains(text(),'itemvalue')]" dataField="false" anchor="true">
		</WebElement>
		<Link name="Tops" type="xpath"
			locator="//a/span[contains(text(),'Men')]/..//following-sibling::ul/*/a/span[contains(text(),'Tops')]" dataField="false" anchor="true">
		</Link>
		<Link name="jackets" type="xpath"
			locator="//a/span[contains(text(),'Men')]/..//following-sibling::ul/*/a/span[contains(text(),'Tops')]/..//following-sibling::ul/*/a/span[contains(text(),'Jackets')]/.." dataField="false" anchor="true">
		</Link>
	</Page>
	
	<Page bearsData="true" name="customerlogin" title="" index="">
		<Link name="CreateAccount" type="linktext"
			locator="Create an Account" dataField="false" anchor="true">
		</Link>		
	</Page>
   
   <Page bearsData="true" name="createcustomeraccount" title="" index="">
		<Textbox name="firstname" type="id"
			locator="firstname" dataField="false" anchor="true">
		</Textbox>
		<Textbox name="lastname" type="id"
			locator="lastname" dataField="false" anchor="true">
		</Textbox>	
		<Textbox name="email" type="id"
			locator="email_address" dataField="false" anchor="true">
		</Textbox>
		<Textbox name="password" type="id"
			locator="password" dataField="false" anchor="true">
		</Textbox>
		<Textbox name="passwordconfirmation" type="id"
			locator="password-confirmation" dataField="false" anchor="true">
		</Textbox>
		<Button name="createaccount" type="xpath"
			locator="//button[@title='Create an Account']/span" dataField="false" anchor="true">
		</Button>
	</Page>
	
	<Page bearsData="true" name="jackets" title="" index="">
