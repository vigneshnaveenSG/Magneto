			locator="//div[contains(text(),'Size')]" dataField="false" anchor="true">
		</Link>	
		<Link name="Medium" type="xpath"
			locator="//div[contains(text(),'Size')]//following-sibling::div//child::div[contains(text(),'M')]" dataField="false" anchor="true">
		</Link>		
		<Link name="price" type="xpath"
			locator="//div[contains(text(),'Price')]" dataField="false" anchor="true">
		</Link>	
		<Link name="FourtyDollorAmount" type="xpath"
			locator="//div[contains(text(),'Price')]//following-sibling::div//child::span[contains(text(),'40')]/.." dataField="false" anchor="true">
		</Link>	
		<Link name="colour" type="xpath"
			locator="//div[contains(text(),'Color')]" dataField="false" anchor="true">
		</Link>	
		<Link name="Red" type="xpath"
			locator="//div[contains(text(),'Color')]//following-sibling::div//child::div[@option-label='Red']" dataField="false" anchor="true">
		</Link>	
		<Link name="material" type="xpath"
			locator="//div[contains(text(),'Material')]" dataField="false" anchor="true">
