			locator="//div[@option-label='Red']" dataField="false" anchor="true">
		</Link>	
		<button name="AddToCartbtn" type="xpath"
			locator="//button[@title='Add to Cart']" dataField="false" anchor="true">
		</button>	
		
		<WebElement name="sizeerror" type="xpath"
			locator="//div[@attribute-code='size']//child::div[contains(text(),'itemvalue')]" dataField="false" anchor="true">
		</WebElement>
		
		<WebElement name="AddToCart" type="xpath"
			locator="//a[contains(text(),'itemvalue')]" dataField="false" anchor="true">
		</WebElement>	
		
		<button name="MyCart" type="xpath"
			locator="//span[contains(text(),'My Cart')]/.." dataField="false" anchor="true">
		</button>
		
		<button name="ProceedtoCheckout" type="xpath"
			locator="//button[contains(text(),'Proceed to Checkout')]" dataField="false" anchor="true">
		</button>	
	</Page>
	
	<Page bearsData="true" name="shipping" title="" index="">
		<Textbox name="emailaddress" type="xpath"
			locator="//input[@id='customer-email'][1]" dataField="false" anchor="true">
