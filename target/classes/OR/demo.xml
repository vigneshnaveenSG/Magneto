		<Link name="fleece" type="xpath"
			locator="//div[contains(text(),'Material')]//following-sibling::div//child::a[contains(text(),'Fleece')]" dataField="false" anchor="true">
		</Link>	
		<Img name="BeaumontJacket" type="xpath"
			locator="//li[@class='item product product-item']//child::img[@alt='Beaumont Summit Kit-M-Red']" dataField="false" anchor="true">
		</Img>	
		<WebElement name="Instock" type="xpath"
			locator="//div[@title='Availability']/span[contains(text(),'In stock')]" dataField="false" anchor="true">
		</WebElement>	
		
